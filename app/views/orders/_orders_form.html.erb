<%= form_for @order, data: { turbo: false }  do |f| %>
  <div class="form-group mb-3">
    <%= f.label :warehouse_id , class:"form-label"%>
     <%= f.collection_select(:warehouse_id, Warehouse.all, :id, :name, {:prompt=>true}, {:class=>'form-select form-select-md'}) %>
  </div>
    <div class="form-group mb-3">
    <%= f.label :supplier_id, class:"form-label" %>
    <%= f.collection_select(:supplier_id, Supplier.all, :id, :corporate_name, {:prompt=>true}, {:class=>'form-select form-select-md'}) %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :estimated_delivery_date, class:"form-label" %>
    <%= f.date_select(:estimated_delivery_date, {}, {:class => 'form-select form-select-md'}) %>


  <div class="form-buttons d-flex gap-2 mt-4">
    <%= f.submit 'Salvar', class:"btn btn-outline-success btn-lg"%>
<%end%>

<%= button_to 'Voltar', product_models_path, method: :get, class:"btn btn-outline-danger btn-lg" %>
</div>
